templates:
  Trakt Franchises:
    trakt_list:
      - https://trakt.tv/users/hthighway/lists/<<list>>?sort=title,asc
    sort_title: +5_<<collection_name>>
    sync_mode: sync
    collection_order: custom
    collection_mode: hide
    file_poster: config/assets/Collections/<<image>>
  TMDB Franchises:
    optional:
      - collections
      - lists
      - movies
      - tmdb_summary
      - summary
    tmdb_collection: <<collections>>
    tmdb_list: <<lists>>
    tmdb_movie: <<movies>>
    tmdb_summary: <<tmdb_summary>>
    summary: <<summary>>
    #url_poster: <<poster>>
    file_poster: config/assets/Collections/<<image>>
    sort_title: +5_<<collection_name>>
    collection_order: release
    collection_mode: hide
  imdb:
    file_poster: config/assets/Collections/<<image>>
    summary: <<summary>>
    sync_mode: sync
    sort_title: +5_<<collection_name>>
    collection_order: release
    collection_mode: hide
